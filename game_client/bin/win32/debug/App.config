<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- ********************以下为框架部分程序的一些配制************************* -->
    <!-- 设备ID -->
    <add key="EquipId" value="02000000000016446714-ef9e-4865-93fe-de91a6720710" />
    <!-- 允许包的大小，如果超出该设定值，将主动断开客户端连接，单位字节, 最大允许4294967292字节, 缺省按最大值 -->
    <add key="AcceptPacketLimit" value="3145728"/>
    <!-- 版本号 -->
    <add key="Version" value="5" />

    <!-- 游戏逻辑 线程池线程数，默认大于等于10 -->
    <add key="GameThreads" value="10" />
    <!-- 登入Web验证地址-->
    <add key="LoginWeb" value="http://api.caihong.com/game/login/clientLogin/" />

    <!-- ********************以下为游戏逻辑部分程序的一些配制************************* -->
  </appSettings>

</configuration>